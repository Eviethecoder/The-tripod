class Spriteplacement extends MusicBeatState
{
    public var testicle:FlxSprite ;
    override function create(){
        var bg:FlxSprite = new FlxSprite(-80).loadGraphic(Paths.image('menuBG'));
        bg.setGraphicSize(Std.int(bg.width * 1.175));
        bg.updateHitbox();
        bg.screenCenter();
        bg.antialiasing = ClientPrefs.globalAntialiasing;
        add(bg);
        testicle = new FlxSprite(0, 0).loadGraphic(Paths.image('test'));
        testicle.screenCenter();
    }

    override public function update(elapsed:Float):Void
        {
            
            super.update(elapsed);
            var xplacement =  FlxG.mouse.getPosition().x;
            var yplacement =  FlxG.mouse.getPosition().y;
            if (FlxG.mouse.pressed)
            {
                
            }
        
            if (FlxG.mouse.justPressed)
            {
                testicle.x =  xplacement;
                testicle.y =  yplacement;          
                trace('test' + xplacement);
            }
        
            if (FlxG.mouse.justReleased)
            {
                
                
            }
        
            
        }

}